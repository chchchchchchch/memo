% http://tex.stackexchange.com/questions/224093/adding-keywords-to-existing-language-for-listings-package

\lstdefinelanguage{Bash+}{%
  language     = Bash,
  morekeywords = { rl,head,tail,ls,grep,
                   cut,tr,rev,wc,md5sum,nawk,mv,
                   cp,rm,pdftk,sort,uniq,convert,composite,
                   java,touch,inkscape,epstopdf,
                   pdf2svg,egrep,shuf,find,
                   autotrace,pdflatex,date,
                   biber,bibtex,makeindex,dot,ps2pdf
        }
}
